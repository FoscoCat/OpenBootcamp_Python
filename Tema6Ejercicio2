class Alumno:
    def __init__(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota
        if self.nota >= 5:
            self.aprobado = True
        else:
            self.aprobado = False

    def __str__(self):
        return f"Nombre: {self.nombre}\nNota: {self.nota}\n{'Aprobado' if self.aprobado else 'Suspendido'}"


alumno = Alumno("Jos√©", 6)
print(alumno)
